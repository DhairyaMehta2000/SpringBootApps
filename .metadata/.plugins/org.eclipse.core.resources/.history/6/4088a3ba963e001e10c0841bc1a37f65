package game;

import java.util.Deque;

import entities.Board;
import entities.Move;
import entities.Player;

public class Game {

	private Board board;
	private Move move;
	private Deque<Player> players;

	public Game(Board board, Move move, Deque<Player> players) {
		super();
		this.board = board;
		this.move = move;
		this.players = players;
	}

}
