package services;

public class PlayerService {

	private String name;
	private char symbol;

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public char getSymbol() {
		return symbol;
	}

	public void setSymbol(char symbol) {
		this.symbol = symbol;
	}

	public PlayerService(String name, char symbol) {
		this.name = name;
		this.symbol = symbol;
	}

}
