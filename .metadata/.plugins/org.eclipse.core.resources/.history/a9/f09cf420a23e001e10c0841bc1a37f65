package entities;

public class Player {

	private String name;
	private char piece;

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public char getPiece() {
		return piece;
	}

	public void setPiece(char piece) {
		this.piece = piece;
	}

	public Player(String name, char piece) {
		super();
		this.name = name;
		this.piece = piece;
	}

	public Player(String name1, char piece1) {
		// TODO Auto-generated constructor stub
		this.name = name1;
		this.piece = piece1;
	}

}
