package entities;

public class Board {

	private int size;
	private char board[][];
	public int getSize() {
		return size;
	}
	public void setSize(int size) {
		this.size = size;
	}
	public char[][] getBoard() {
		return board;
	}
	public void setBoard(char[][] board) {
		this.board = board;
	}
	public Board(int size, char[][] board) {
		super();
		this.size = size;
		this.board = board;
	}
	
	
	
}
